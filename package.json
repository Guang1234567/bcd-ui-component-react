{
  "name": "bcd-ui-component-react",
  "sideEffects": false,
  "version": "0.0.3",
  "private": false,
  "description": "React specific wrapper for bcd-ui-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/guang1234567/bcd-ui-component-react.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
